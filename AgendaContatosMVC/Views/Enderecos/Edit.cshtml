@model AgendaContatosMVC.Models.Endereco

@{
    ViewData["Title"] = "Editar Endereço";
}

<h2>@ViewData["Title"]</h2>


<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="Enderecos" class="control-label"></label>
                <input asp-for="Enderecos" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="ContactId" class="control-label"></label>
                <input asp-for="ContactId" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>